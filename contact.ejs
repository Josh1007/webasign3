<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>

<link rel="stylesheet" type="text/css" href="style.css">
<main>

<div class="container">
  <h1>Contact Form</h1>
  <form action="/action_page.php">
    <label for="fname">Name</label>
    <h1 id="Contact"></h1>
 

    <input type="text" id="fname" name="firstname" placeholder="Your name..">
    <label id="tel"><b>Phone Number</b></label>
    <input type="tel" placeholder="Enter Phone Number" name="tel" pattern="[0-9]{3}[0-9]{3}[0-9]{4}" required>
    <label id="email"><b>Email</b></label>
    <input type="email" placeholder="Enter Email" name="email" required><br><br>
    <label for="subject">Questions</label>
    <textarea id="subject" name="subject" placeholder="Questions or if you just want to reach out" style="height:200px"></textarea>

    <input type="submit" value="Submit">
  </form>
</div>

</main>
